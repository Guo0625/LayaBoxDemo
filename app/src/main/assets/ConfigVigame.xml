    <!--<Permissions></Permissions>-->
    <IsSDK>true</IsSDK>
    <HealthEnable>true</HealthEnable>
    <HealthColor>FFFFFFFF</HealthColor>
    <!--index 和 company 对应-->
    <!--1.动能无线
    2.飞鸟与鱼
    3.统掌科技
    4.唯变科技
    5.全支付科技
    6.漫城科技-->
    <CompanyIndex>1</CompanyIndex><!--公司名称对应的数字，对应关系见上面-->
